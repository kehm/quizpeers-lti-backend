{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "import/extensions": "off",
        "quote-props": "off",
        "prefer-destructuring": "off",
        "no-shadow": "warn",
        "no-empty": "warn",
        "no-unused-vars": "warn",
        "no-param-reassign": "warn",
        "no-underscore-dangle": "warn"
    }
}
